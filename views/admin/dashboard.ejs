    <div class="card my-4">
        <div class="card-header">
            <i class="fas fa-table me-1"></i>
            Bağış Tablosu
        </div>
        <div class="card-body">
            <table id="datatablesSimple">
                <thead>
                    <tr>
                        <th>İsim Soyisim</th>
                        <th>Ürün Id</th>
                        <th>E-Posta</th>
                        <th>Telefon</th>
                        <th>İşlem Tarihi</th>
                        <th>Tutar</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>İsim Soyisim</th>
                        <th>Ürün Id</th>
                        <th>E-Posta</th>
                        <th>Telefon</th>
                        <th>İşlem Tarihi</th>
                        <th>Tutar</th>
                    </tr>
                </tfoot>
                <tbody>
                    <% found.forEach((call)=>{ %>
                        <tr>
                            <td><%= call.name %> <%= call.surname %></td>
                            <td><%= call.productId %></td>
                            <td><%= call.email %></td>
                            <td><%= call.phone %></td>
                            <td><%= call.date %></td>
                            <td><%= call.paidPrice %> ₺</td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>
